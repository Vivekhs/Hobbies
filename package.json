{
    "name": "hobbies",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "node ./bin/www",
        "start:dev": "set NODE_ENV=DEV&& node ./bin/www",
        "start:prod": "set NODE_ENV=PROD&& node ./bin/www",
        "build": "del-cli dist/*  && ng build --prod && del-cli public/assets/javascripts/* && node config/gulp.prod.js",
        "dev:build": "del-cli public/assets/javascripts/*  && concurrently \"ng build -op public/assets/javascripts/ -w\" \"node config/gulp.dev.js\"",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "dependencies": {
        "@angular/animations": "~5.1.3",
        "@angular/common": "~5.1.3",
        "@angular/compiler": "~5.1.3",
        "@angular/core": "~5.1.3",
        "@angular/forms": "~5.1.3",
        "@angular/http": "~5.1.3",
        "@angular/platform-browser": "~5.1.3",
        "@angular/platform-browser-dynamic": "~5.1.3",
        "@angular/router": "~5.1.3",
        "body-parser": "~1.18.2",
        "concurrently": "^3.5.1",
        "cookie-parser": "~1.4.3",
        "core-js": "~2.5.3",
        "debug": "~2.6.9",
        "express": "~4.15.5",
        "gulp": "~3.9.1",
        "gulp-concat": "~2.6.1",
        "gulp-rename": "~1.2.2",
        "gulp-sass": "~3.1.0",
        "morgan": "~1.9.0",
        "node-sass": "~4.7.2",
        "pug": "2.0.0-beta11",
        "reflect-metadata": "~0.1.10",
        "rxjs": "~5.5.6",
        "serve-favicon": "~2.4.5",
        "zone.js": "~0.8.19"
    },
    "devDependencies": {
        "@angular/cli": "1.6.3",
        "@angular/compiler-cli": "~5.1.3",
        "@angular/language-service": "~5.1.3",
        "@types/jasmine": "~2.5.53",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "~6.0.60",
        "codelyzer": "~4.0.1",
        "del-cli": "~1.1.0",
        "jasmine-core": "~2.6.2",
        "jasmine-spec-reporter": "~4.1.0",
        "karma": "~1.7.0",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "~1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "~0.2.2",
        "protractor": "~5.1.2",
        "ts-node": "~3.2.0",
        "tslint": "~5.7.0",
        "typescript": "~2.4.2"
    }
